{
    "manifest_version": 2,
    "name": "funkyfirefox",
    "version": "1.0",
  
    "description": "enhance your browsing experience",
    "homepage_url": "https://github.com/jostbem/funkyfirefox",
    "icons": {
      "48": "icons/goofy-ahhhh.png"
    },
  
    "permissions": ["activeTab"],
  
    "browser_action": {
      "default_icon": "icons/goofy.png",
      "default_title": "funkyfirefox",
      "default_popup": "popup/settings.html"
    }
  
  }
  